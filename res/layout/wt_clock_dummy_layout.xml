<?xml version="1.0" encoding="utf-8"?>

<com.cj.clock.WtClockRoot 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:clock="http://schemas.android.com/apk/res/com.cj.wtlauncher"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wt_clock_1_dial">

<com.cj.clock.WtClock
    android:id="@+id/clk_hour2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    
    clock:clkstyle="pointer"
    clock:value_min="0"
    clock:value_max="359"
    clock:pointer_drawable="@drawable/wt_clock_1_hour"
    clock:pointer_center_x="160"
    clock:pointer_center_y="161">
</com.cj.clock.WtClock>

<com.cj.clock.WtClock
    android:id="@+id/clk_min2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    
    clock:clkstyle="pointer"
    clock:value_min="0"
    clock:value_max="59"
    clock:pointer_drawable="@drawable/wt_clock_1_min"
    clock:pointer_center_x="160"
    clock:pointer_center_y="160">
</com.cj.clock.WtClock>

<com.cj.clock.WtClock
    android:id="@+id/clk_wifi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    
    clock:clkstyle="pointer"
    clock:value_min="0"
    clock:value_max="59"
    clock:pointer_drawable="@drawable/wt_clock_1_min"
    clock:pointer_center_x="160"
    clock:pointer_center_y="160">
</com.cj.clock.WtClock>

<com.cj.clock.WtClock
    android:id="@+id/clk_bt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    
    clock:clkstyle="pointer"
    clock:value_min="0"
    clock:value_max="59"
    clock:pointer_drawable="@drawable/wt_clock_1_min"
    clock:pointer_center_x="160"
    clock:pointer_center_y="160">
</com.cj.clock.WtClock>

<TextView 
    android:id="@+id/digit_month"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/tx_clk_ampm"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/clk_batt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/clk_message"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/clk_dial"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/clk_wifi_connect"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/clk_mobile_signal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

<TextView 
    android:id="@+id/clk_mobile_data"
    android:layout_width="match_parent"
    android:layout_height="match_parent"    />

</com.cj.clock.WtClockRoot>