<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="AnalogClock">
        <attr name="dial" format="reference"/>
        <attr name="hand_hour" format="reference"/>
        <attr name="hand_minute" format="reference"/>
        <attr name="center_point" format="reference"/>
        <attr name="week_point_drawable" format="reference"/>
        <attr name="week_point_center_x" format="integer"/>
        <attr name="week_point_center_y" format="integer"/>
    </declare-styleable>
    <declare-styleable name="DigitalClock">
        <attr name="digit_drawables" format="reference"/>
        <attr name="digit_colon_drawable" format="reference"/>
        <attr name="date_text_color" format="reference|color"/>
    </declare-styleable>
